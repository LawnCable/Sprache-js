{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "member-access": false,
        "variable-name": false,
        "quotemark": false,
        "arrow-parens": false,
        "interface-name": false,
        "trailing-comma": false,
        "no-unnecessary-initializer": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "max-line-length": [true, 160]
    },
    "rulesDirectory": []
}
